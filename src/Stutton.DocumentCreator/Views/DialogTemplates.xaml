<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Stutton.DocumentCreator.Views"
                    xmlns:dialogVms="clr-namespace:Stutton.DocumentCreator.ViewModels.Dialogs"
                    xmlns:dialogs="clr-namespace:Stutton.DocumentCreator.Views.Dialogs">

    <DataTemplate DataType="{x:Type dialogVms:MessageDialogViewModel}">
        <dialogs:MessageDialog />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogVms:AddWorkItemExpressionDialogViewModel}">
        <dialogs:AddWorkItemExpressionDialog />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogVms:TfsUrlDialogViewModel}">
        <dialogs:TfsUrlDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogVms:AddTemplateFieldDialogViewModel}">
        <dialogs:AddTemplateFieldDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogVms:AddTemplateAutomationDialogViewModel}">
        <dialogs:AddTemplateAutomationDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogVms:MaterialOpenFileDialogViewModel}">
        <dialogs:MaterialOpenFileDialog/>
    </DataTemplate>

</ResourceDictionary>